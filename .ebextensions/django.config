option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "scbi.settings"
    "PYTHONPATH": "/opt/python/current/app/scbi:$PYTHONPATH"
    "ALLOWED_HOSTS": ".elasticbeanstalk.com"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: scbi/scbi/wsgi.py
    NumProcesses: 3
    NumThreads: 20